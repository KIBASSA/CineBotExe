    <add key="person-page-index" value="1" />
    <add key="person-page-alphabet" value="A" />
    <add key="cinema-page-index" value="1" />
    <add key="series-page-index" value="1" />
    <add key="entity-to-launch" value="cinemaalwaysshowing" />
